  },
  "dependencies": {
    "react": "~15.4.1",
    "react-dom": "~15.4.1"
  },
  "devDependencies": {
    "uglifyjs": "~2.4.10",
    "babelify": "~7.3.0",
    "watchify": "~3.7.0",
    "browserify": "~13.1.1"
  },
  "browserify":{"transform":[["babelify"]]}
}
